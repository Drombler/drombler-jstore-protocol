{
  "$id": "http://www.drombler.org/drombler-jstore-protocol/ApplicationVersionSearchResponse.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "applicationVersionInfos": {
      "$id": "/properties/applicationVersionInfos",
      "type": "array",
      "items": {
        "$id": "/properties/applicationVersionInfos/items",
        "type": "object",
        "properties": {
          "applicationId": {
            "$id": "/properties/applicationVersionInfos/items/properties/applicationId",
            "type": "object",
            "$ref": "Definitions.json#/definitions/applicationId"
          },
          "applicationVersion": {
            "$id": "/properties/applicationVersionInfos/items/properties/applicationVersion",
            "type": "string",
            "title": "The newest application version",
            "default": "",
            "examples": [
              "1.0.0"
            ]
          },
          "jreInfo": {
            "$id": "/properties/applicationVersionInfos/items/properties/jreInfo",
            "type": "object",
            "properties": {
              "jreVendorId": {
                "$id": "/properties/applicationVersionInfos/items/properties/jreInfo/properties/jreVendorId",
                "type": "string",
                "title": "The JRE vendor id",
                "default": "",
                "examples": [
                  "drombler"
                ]
              },
              "javaMajorVersion": {
                "$id": "/properties/applicationVersionInfos/items/properties/jreInfo/properties/javaMajorVersion",
                "type": "string",
                "title": "The Java major version",
                "default": "",
                "examples": [
                  "8",
                  "9",
                  "10"
                ]
              },
              "customJREId": {
                "$id": "/properties/applicationVersionInfos/items/properties/jreInfo/properties/customJREId",
                "type": "string",
                "title": "The custom JRE id",
                "description": "The vendor specific JRE id for custom JREs.",
                "default": "",
                "examples": [
                  "drombler-base"
                ]
              },
              "modules": {
                "$id": "/properties/applicationVersionInfos/items/properties/jreInfo/properties/modules",
                "type": "array",
                "title": "The Java modules",
                "description": "The required Java modules, which should be respected by alternate JREs.",
                "default": "",
                "examples": [
                  "java.base"
                ],
                "items": {
                  "$id": "/properties/applicationVersionInfos/items",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}